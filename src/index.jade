doctype html
html
  head
    meta(charset='utf-8')
    title Digital Inove
    meta(name='description', content='Gráfica Rápida e Papelaria')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(href='css/main.css', media='all', rel='stylesheet')
    link(href='css/normalize.css', media='all', rel='stylesheet')
  body
    #jumbo
      .bars
        - each i in [1, 2, 3, 4]
          .bar
      .content
        .logo
          div
            img(src='images/logo-digital-inove.svg', width='100%', alt='Digital Inove')
            h2 Gráfica Rápida e Papelaria
        .info
          .info-content
            .contact
              span.phone (12) 3522-0086
            .actions
              a.btn(href='#serviços') Veja nossos serviços
              a.btn(href='#contatos') + contatos / endereço
    script(src='js/main.js')
